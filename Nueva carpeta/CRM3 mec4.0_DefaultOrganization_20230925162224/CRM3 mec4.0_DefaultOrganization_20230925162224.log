        query = "SELECT COUNT(*) as total_customers, SUM(satisfaction) as total_satisfaction FROM customers"
        self.cursor.execute(query)
        result = self.cursor.fetchone()
        return result

    def close_connection(self):
        self.cursor.close()
        self.connection.close()
